{
    "name": "czim/laravel-nestedupdater",
    "description": "Eloquent model updater for nested relations data for Laravel.",
    "keywords": [
        "eloquent",
        "model",
        "relations"
    ],
    "homepage": "https://github.com/czim/laravel-nestedupdater",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen.zimmerman@endeavour.nl",
            "homepage": "https://github.com/czim",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.9",
        "orchestra/testbench": "^7.0",
        "orchestra/database": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\NestedModelUpdater\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\NestedModelUpdater\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
