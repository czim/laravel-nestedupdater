{
    "name": "czim/laravel-nestedupdater",
    "description": "Eloquent model updater for nested relations data for Laravel.",
    "keywords": [
        "eloquent",
        "model",
        "relations"
    ],
    "homepage": "https://github.com/czim/laravel-nestedupdater",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pxlwidgets.com",
            "homepage": "https://github.com/czim",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^3.0|^4.0",
        "orchestra/database": "^3.0|^4.0",
        "php-coveralls/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\NestedModelUpdater\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\NestedModelUpdater\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
