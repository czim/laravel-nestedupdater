{
    "name": "czim/laravel-nestedupdater",
    "description": "Eloquent model updater for nested relations data for Laravel.",
    "keywords": [
        "eloquent",
        "model",
        "relations"
    ],
    "homepage": "https://github.com/czim/laravel-nestedupdater",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pixelindustries.com",
            "homepage": "http://www.pixelindustries.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "3.3.*"
    },
    "autoload": {
        "psr-4": {
            "Czim\\NestedModelUpdater\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\NestedModelUpdater\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
